{
    "name": "b-eventb-language-extension",
    "displayName": "-beventb language extension",
    "description": "compiler support for b and eventb via prob",
    "version": "0.0.1",
    "publisher": "SeeBasTStick",
    "icon": "prob_logo.png",
    "author": "Sebastian Stock",
	"repository": {
		"type": "git",
		"url": "https://github.com/SeeBasTStick/b-eventb-language-extension"
	},
    "engines": {
        "vscode": "^1.45.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "b",
            "aliases": ["B", "b"],
            "extensions": [".mch",".imp",".ref",".eventb"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "b",
            "scopeName": "",
            "path": "./syntaxes/b.tmLanguage.json"
        }]
    }
}